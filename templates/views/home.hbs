<body class="container full-height-grow">
   <!-- Card -->
   <div id="mainBody" class="card weather-card">
      <header>
         <nav class="navbar  display-block">
            <!-- Title -->
            <div class="lead">
               <h4 class="card-title font-weight-bold">Weather</h4>
               <h4 class="card-title font-weight-bold">Forecast</h4>
            </div>

            <form id="form" class="form-inline submit_form ">
               <div class="md-form  submit_form">
                  <input type="text" id="form1" class="form-control input">
