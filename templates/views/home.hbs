                  <p class="card-text mb-2" id="message2"></p>

               </div>
            </section>
            <!--Section: Content-->
         </div>

         <footer class="">
            <div id="footer_container" class="d-flex justify-content-between">
               <p><i class="fas fa-tint fa-lg text-info p-4"></i>3% Precipitation</p>
